{
    "name": "201created/ddd",
    "description": "DDD framework used in 201Created projects",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "0.1.5",
    "authors": [
        {
            "name": "Christoph Hautzinger",
            "email": "ch@201created.de"
        },
        {
            "name": "Marko Vujnovic",
            "email": "ms@201created.de"
        }
    ],
    "require": {
        "201created/collections": "^0.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "ramsey/uuid": "^4.0",
        "sensio/framework-extra-bundle": "^5.5",
        "simple-bus/message-bus": "^3.0",
        "simple-bus/symfony-bridge": "^5.3",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/serializer": "^5.0",
        "tightenco/collect": "^7.4",
        "webmozart/assert": "^1.7"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "C201\\Ddd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C201\\Ddd\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:c201/201created-collections.git"
        }
    ]
}
